        </activity>
        <meta-data
            android:name="com.crashlytics.ApiKey"
            android:value="1d381e3ec0ee38934c5cc924d389503437293d83" />
        <activity
            android:name="com.mopub.mobileads.MoPubActivity"
            android:configChanges="keyboardHidden|orientation|screenSize" />
